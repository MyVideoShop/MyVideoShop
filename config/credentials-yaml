# Anmeldedaten für verschiedene Webseiten
# Die Passwörter werden automatisch verschlüsselt

users:
  # Haupt-Benutzer
  default:
    websites:
      main:
        email: "deine-email@example.com"
        password: "PLAIN:dein-passwort"  # Beim ersten Start hier Passwort eintragen
        
  # Weitere Benutzer können hier hinzugefügt werden
  # user2:
  #   websites:
  #     main:
  #       email: "user2@example.com"
  #       password: "PLAIN:passwort2"
